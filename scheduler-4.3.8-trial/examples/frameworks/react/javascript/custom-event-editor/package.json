    "@date-io/date-fns": "~1.3.9",
    "@material-ui/core": "~4.4.0",
    "@material-ui/icons": "~4.2.1",
    "@material-ui/pickers": "~3.2.4",
    "cross-env": "~7.0.2",
    "date-fns": "~2.0.1",
    "react": "~16.14.0",
    "react-dom": "~16.14.0",
    "react-scripts": "~3.4.4"
  },
  "devDependencies": {
    "postinstall": "~0.5.1",
    "sass": "~1.42.0"
